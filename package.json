{
  "name": "easy-front-express-api",
  "version": "0.0.0",
  "description": "Easy Front Express API",
  "private": true,
  "scripts": {
    "start": "node dist",
    "prestart": "npm run build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "rimraf dist && babel src -D --out-dir dist",
    "prettier": "prettier --write **/*.{js,json,yml,yaml,md}",
    "dev": "nodemon --watch .env --watch src --exec babel-node src --source-maps"
  },
  "dependencies": {
    "ali-oss": "^6.1.1",
    "async": "^3.0.0",
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-redis": "^3.4.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "csurf": "^1.10.0",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "ejs": "~2.6.1",
    "express": "^4.16.4",
    "express-rate-limit": "^4.0.1",
    "express-session": "^1.16.1",
    "helmet": "^3.18.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.11",
    "log4js": "^4.2.0",
    "morgan": "~1.9.1",
    "node-rsa": "^1.0.5",
    "redis": "^2.8.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "lint-staged": "^8.1.7",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  }
}
